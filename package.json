{
  "name": "angular-17-jwt-auth-starter-standalone",
  "config": {
    "app": "angular-17-jwt-auth-starter-standalone",
    "repo": "uportal"
  },
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "node --max_old_space_size=64000 node_modules/@angular/cli/bin/ng build",
    "lint": "ng lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss}\"",
    "prettier:format": "prettier --write \"{,src/{app,environments}/**/}*.{md,json,ts,css,scss,yml}\"",
    "remove-unsed-import": "node ./node_modules/tslint/bin/tslint --config tslint-import.json --fix --project .",
    "docker-login": "cross-conf-env docker login -u uportal",
    "docker-logout": "cross-conf-env docker logout",
    "docker-build-latest": "cross-conf-env docker build -f Dockerfile -t npm_package_config_repo/npm_package_config_app:latest .",
    "docker-push-latest": "cross-conf-env docker push npm_package_config_repo/npm_package_config_app:latest",
    "docker-tag-latest-to-prod": "cross-conf-env docker tag npm_package_config_repo/npm_package_config_app:latest npm_package_config_repo/npm_package_config_app:npm_package_version",
    "docker-push-prod": "cross-conf-env docker push npm_package_config_repo/npm_package_config_app:npm_package_version",
    "docker-compose-up": "cross-conf-env docker compose up",
    "docker-compose-down": "cross-conf-env docker compose down",
    "docker-compose-up-local": "cross-conf-env docker compose -f docker-compose-local.yml up",
    "docker-compose-down-local": "cross-conf-env docker compose -f docker-compose-local.yml down",
    "var": "cross-conf-env echo  npm_package_config_repo/npm_package_config_app:npm_package_version",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.1",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.2.1",
    "@angular/compiler": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/forms": "^17.2.1",
    "@angular/platform-browser": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@angular/router": "^17.2.1",
    "@fullcalendar/angular": "^6.0.3",
    "@fullcalendar/core": "^6.0.3",
    "@fullcalendar/daygrid": "^6.0.3",
    "@fullcalendar/interaction": "^6.0.3",
    "@fullcalendar/timegrid": "^6.0.3",
    "bootstrap": "^4.6.2",
    "chart.js": "^3.3.2",
    "primeflex": "^3.3.1",
    "primeicons": "6.0.1",
    "primeng": "17.2.0",
    "prismjs": "^1.29.0",
    "quill": "^1.3.7",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "~17.2.0",
    "@angular/compiler-cli": "^17.2.1",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.22",
    "cross-conf-env": "^1.3.0",
    "jasmine-core": "~5.1.0",
    "karma": "^6.4.2",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "3.2.5",
    "typescript": "~5.3.3"
  }
}
